torus
0 0 0 25 150
#rotate 90 degrees about y to check lines
ident
rotate
y 90
move
250 250 0
apply
#reset torus, rotate 90 degrees about x to check lines
ident
move
-250 -250 0
rotate
y -90
rotate
x 90
move
250 250 0
apply
#reset torus, rotate to make it look cool
ident
move
-250 -250 0
rotate
x 70
rotate
y 20
move
250 250 0
apply
